public class no1{
	public static void main(String args[]){
		int n = 5;
		int m = 7;
		int res = 0;
		int res1 = 0;
		public int factorial(int x){
			int temp = 0;
			if(x > 0){
				temp = factorial(x - 1);
			}
			else{
				temp = x;
			}
			return(temp);
		}
		res = res + factorial(n);
		System.out.print(res);
		System.out.println();
		res1 = res1 + factorial(m);
		System.out.print(res1);
	}
}
