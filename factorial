public class no1{
	public static void main(String args[]){
		int n = 5;
		int m = 7;
		public int factorial(int x){
			int temp = 0;
			if(x > 0){
				temp = factorial(x - 1);
			}
			else{
				temp = x;
			}
			return(temp);
		}
		System.out.print(factorial(n));
		System.out.println();
		System.out.print(factorial(m));
	}
}
